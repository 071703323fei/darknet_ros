<launch>
  <node pkg="darknet2_rsl" type="darknet2_rsl" name="darknet2_rsl" output="screen">
    <param name="cfg_path" 		value="$(find darknet2_rsl)/cfg/tiny-yolo-voc.cfg" />
    <param name="weights_path" 		value="$(find darknet2_rsl)/weights/tiny-yolo-voc.weights" />
    <param name="camera_topic_name" 	value="/camera/image_raw" />
    <param name="object_threshold" 	value="0.2" />
  </node>
</launch>
